.machine_wp {
  display: flex;
  width: 80%;
  margin: 23px auto;
}

.machine_wp .machine_item_wp {
  width: 42px;
  height: 70px;
  position: relative;
  overflow: hidden;
  text-align: center;
}

.machine_wp .machine_item_wp:last-child {
  margin-right: 5;
}

.machine_wp .machine_item_wp .machine_item_slide {
  position: absolute;
  width: 40px;
  height: 70px;
  top: 0;
  left: 0;
}

.machine_wp .machine_item_wp .machine_item_ele {
  width: 40px;
  height: 70px;
  line-height: 70px;
  font-size: 30px;
  color: white;
  text-align: center;
}
